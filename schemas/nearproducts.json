{
    "rawquery" : {
        "type":"procedure",
        "parameters" : ["page","size","search"],
        "query": "call procuctsearch(\"$page\",\"$size\",\"$search\")"
    },
    "fields" : [
        {
            "fieldName":"itemid",
            "dataType":"int"
        },
        {
            "fieldName":"name",
            "dataType":"java.lang.String"
        },
        {
            "fieldName":"caption",
            "dataType":"java.lang.String"
        },
        {
            "fieldName":"keywords",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 500
            }
        },
        {
            "fieldName":"price",
            "dataType":"float"
        },
        {
            "fieldName":"discountper",
            "dataType":"float"
        },
        {
            "fieldName":"currencycode",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 4
            }
        },
        {
            "fieldName":"imgurl",
            "dataType":"java.lang.String"
        },
        {
            "fieldName":"catogory",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 40
            }
        },
        {
            "fieldName":"uom",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 100
            }
        },
        {
            "fieldName":"invType",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 10
            }
        },
        {
            "fieldName":"status",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 10
            }
        },
        {
            "fieldName":"collectedByID",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 100
            }
        },
        {
            "fieldName":"collectedBy",
            "dataType":"java.lang.String",
            "annotations":{
                "maxLen": 200
            }
        }          
    ]
}
